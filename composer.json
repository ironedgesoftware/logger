{
    "name": "ironedge/logger",
    "description": "A very simple factory to easily instantiate loggers from different vendors (monolog, for example) using config arrays.",
    "keywords": ["logger", "frenzy", "framework"],
    "homepage": "http://www.ironedgesoftware.com",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name":         "Gustavo Falco",
            "email":        "comfortablynumb84@gmail.com",
            "homepage":     "http://www.ironedgesoftware.com/"
        }
    ],
    "support": {
        "issues": "https://github.com/ironedgesoftware/logger/issues"
    },
    "require": {
        "php":                          "^7.0",

        "monolog/monolog":              "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit":              "^4.5 || ^5.0.5",
        "phpunit/phpunit-mock-objects": "2.3.0 || ^3.0"
    },
    "autoload": {
        "psr-4": {
            "IronEdge\\Component\\Logger\\": "src",
            "IronEdge\\Component\\Logger\\Test\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}